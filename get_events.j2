<body>
<h1>Filmy</h1>
<h2>{{ date }}</h2>
<table id="repertuar">
    <tbody>
    <tr>
        <th>Godzina</th>
        <th>Tytu≈Ç</th>
        <th>Link</th>
    </tr>

    {% for event in events.values() %}
        <tr>
            <td>{{ event.hour }}</td>
            <td>{{ event.film.name }}</td>
            <td><a href="{{ event.bookingLink }}">Rezerwuj</a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>

